{
    "manifest_version": 3,
    "name": "Gandalf - Browser History Recap",
    "version": "1.0.0",
    "description": "Get meaningful insights from your browsing patterns",
    "permissions": [
        "history",
        "tabs",
        "storage",
        "alarms",
        "notifications"
    ],
    "action": {
        "default_icon": {
            "16": "icons/icon16.png",
            "48": "icons/icon48.png",
            "128": "icons/icon128.png"
        }
    },
    "chrome_url_overrides": {},
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }
}